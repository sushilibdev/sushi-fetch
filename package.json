{
  "name": "sushi-fetch",
  "version": "0.2.0",
  "description": "ðŸ£ A tiny but powerful data-fetching & caching library for modern JavaScript & TypeScript apps",
  "keywords": [
    "fetch",
    "http",
    "cache",
    "data-fetching",
    "typescript",
    "javascript",
    "api",
    "client",
    "request",
    "swr",
    "deduplication",
    "retry",
    "lightweight"
  ],
  "homepage": "https://github.com/sushilibdev/sushi-fetch",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sushilibdev/sushi-fetch.git"
  },
  "bugs": {
    "url": "https://github.com/sushilibdev/sushi-fetch/issues"
  },
  "author": {
    "name": "sushilibdev",
    "email": "sushilibdev@gmail.com"
  },
  "license": "MIT",
  "type": "module",
  
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean --minify",
    "watch": "tsup src/index.ts --format cjs,esm --dts --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "test": "vitest run",
    
    "example:vanilla": "ts-node examples/vanilla-js/app.ts",
    "example:express": "ts-node examples/express/server.ts",
    "example:node-cli": "ts-node examples/node-cli/app.ts",
    "example:bun": "bun run examples/bun/app.ts",
    
    "prepublishOnly": "npm run typecheck && npm run build"
  },
  
  "dependencies": {},
  
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  }
}